{
  "name": "nurse-joy-hackathon-server",
  "version": "0.0.1",
  "private": true,
  "publishConfig": {
    "access": "restricted"
  },
  "type": "module",
  "engines": {
    "node": ">=16"
  },
  "main": "dist/index",
  "scripts": {
    "start": "node .",
    "build": "tsc",
    "watch": "pnpm build -- -w",
